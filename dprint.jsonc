{
  "$schema": "https://dprint.dev/schemas/v0.json",

  // TypeScript / JavaScript configuration closest to prettier --print-width 120
  "typescript": {
    "lineWidth": 120
  },
  "markdown": {},

  "includes": ["src/*.js", "src/utils/*.js", "src/*.md"],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.95.11.wasm",
    "https://plugins.dprint.dev/markdown-0.19.0.wasm",
  ]
}
